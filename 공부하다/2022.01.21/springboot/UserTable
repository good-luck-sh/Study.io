
CREATE TABLE KAKAO_LOGIN_USERS
   (   USER_ID       VARCHAR2(20 BYTE), 
   USER_NAME       VARCHAR2(100 BYTE), 
   USER_PASSWORD       VARCHAR2(20 BYTE), 
   USER_EMAIL       VARCHAR2(255 BYTE), 
   USER_AGE       NUMBER(3,0), 
   USER_GENDER       VARCHAR2(10 BYTE), 
   USER_LOGIN_TYPE    VARCHAR2(20 BYTE), 
   USER_POINT       NUMBER(10,0)       DEFAULT 0, 
   USER_DISABLED       CHAR(1 BYTE)       DEFAULT 'N', 
   USER_UPDATED_DATE    DATE, 
   USER_CREATED_DATE    DATE          DEFAULT sysdate
   )
