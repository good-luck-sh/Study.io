package example_09_10_static;

public class Sample2App {

	public static void main(String[] args) {//진입점이 되는 메인이 설계되면 바로 사용이 가능해야하기 때문에 main이 static메소드였음
		Sample2 s1 = new Sample2();
		s1.increase();
		s1.increase();
		s1.increase();
		s1.increase();
		s1.increase();
		s1.printValue();
		Sample2 s2 = new Sample2();
		s2.increase();
		s2.printValue();
		Sample2 s3 = new Sample2();
		s3.increase();
		s3.printValue();
		Sample2 s4 = new Sample2();
		s4.increase();
		s4.printValue();
		

	}}
