package example_08_31_var;

public class Sample6 {
		public static void main(String[] args) {
			/*
			 * 연습1 
			 * 변수를 선언하고 값을 출력해보기 
			 * 자신의 이름, 나이, 키 , 몸무게, 혈액형, 전화번호 , 이메일, 우편번호, 주소를  
			 * 변수에 저장하고, 저장된 값을 출력한다. 
			 */
			String name = "kimsounghee";
			int age = 31;
			double height = 172.0;
			double weight = 65.7;
			//비교, 어느 값을 찾을 수 있음, BMI지수를 계산할 수 있다.
			
			
			String bloodType = "RH+ A";
			String phoneNumber = "010-1234-5670";//tel으로 입력하도록
			String email = "doongland@gmail.com";
			String adress = "00000";//우편번호를 가지고 연산을 하지 않기 때문에 문자열로 입력한다.사칙연산안하면 문자열로 함
			String homeAdress = "서울시 ";
			
			
			System.out.println("자신의 이름, 나이, 키 , 몸무게, 혈액형, 전화번호, 이메일, 우편번호");
			System.out.println("주소를 출력하세요");
			System.out.println(name);
			System.out.println(age);
			System.out.println(height);
			System.out.println(weight);
			System.out.println(bloodType);
			System.out.println(phoneNumber);
			System.out.println(email);
			System.out.println(adress);
			System.out.println(homeAdress);
		
			
			
		}

}
